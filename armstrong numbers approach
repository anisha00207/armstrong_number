 #include <iostream> 
   #include <cmath>
   using namespace std;
   int main()
   { int n,digit,sum=0,a,count=0,b;
   cin >>n;
   a=n;
   b=n;
   while(n>0){
   digit=n%10;
   count++;
 
   n=n/10;
   
   }
   
    while(a>0){
   digit=a%10;
   sum=sum+ pow(digit,count);
 
   a=a/10;
   
   }
   
         if(sum==b){
           cout<<b<<" is an armstrong number";
       }
       else{
           cout<<b<<" is not an armstrong number";
       }
       
     
       return 0;
   }
